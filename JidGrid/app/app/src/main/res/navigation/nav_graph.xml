<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LocatorFragment">

    <fragment
        android:id="@+id/LocatorFragment"
        android:name="com.example.jidgrid.LocatorFragment"
        android:label="@string/maps_fragment_label"
        tools:layout="@layout/fragment_locator">

        <action
            android:id="@+id/action_LocatorFragment_to_MapsFragment"
            app:destination="@id/MapsFragment" />
    </fragment>
    <fragment
        android:id="@+id/MapsFragment"
        android:name="com.example.jidgrid.MapsFragment"
        android:label="@string/locator_fragment_label"
        tools:layout="@layout/fragment_maps">

        <action
            android:id="@+id/action_MapsFragment_to_LocatorFragment"
            app:destination="@id/LocatorFragment" />
    </fragment>
</navigation>